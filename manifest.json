{
    "manifest_version": 3,
    "name": "Tab Switcher",
    "version": "1.0",
    "description": "Quickly switch tabs with a search bar, like VS Code's Ctrl+P.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon-128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "open_tab_switcher": {
        "suggested_key": {
          "default": "Ctrl+Shift+Q"
        },
        "description": "Open the Tab Switcher"
      }
    }
  }
  